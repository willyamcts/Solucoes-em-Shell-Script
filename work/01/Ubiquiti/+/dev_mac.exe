# [arg1] com senha de acesso
# [arg2] com nome do usuario de acesso
# [arg3] com IP de acesso
# 	sshpass -p $1 ssh -p 22 $2@$3 .... 

#########################  ##############################
#########################################################

# 1- verifica modelo do eq. (/etc/board.info - linha board.name)
#	cat /etc/board.info | grep board.name | awk -F= "'{print '$2'}'"
#	cat /etc/board.info | grep board.name | cut -d= -f2

# 2- verifica modelo do eq. (/etc/board.info - linha board.hwaddr)
# 3- adiciona a um arquivo o MAC e EQ.



startAccess() {

sshpass -p $2 ssh -p 22 -o "StrictHostKeyChecking no" $1@$3 'echo "`cat /etc/board.info | grep board.hwaddr | cut -d= -f2` \
	`cat /etc/board.info | grep board.name | cut -d= -f2`"'

}

# Return [MAC]	[Modelo.Eq.]
